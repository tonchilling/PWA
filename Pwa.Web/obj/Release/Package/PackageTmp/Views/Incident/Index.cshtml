    function GetFollower(jsonData, incidentNo) {
        $("#tb_IncidentNo").val(incidentNo);
        var no = 0;
        $.each(jsonData, function (index, item) {
            no = no + 1;
        });
        $("#tb_No").val(no + 1);
        var today = new Date();
        var dd = today.getDate();
        var mm = today.getMonth() + 1;

        var yyyy = today.getFullYear();
        if (dd < 10) {
            dd = '0' + dd;
        }
        if (mm < 10) {
            mm = '0' + mm;
        }
        if (yyyy < 2500) {
            yyyy = yyyy + 543;
        }
        var now = dd + '/' + mm + '/' + yyyy;
        $("#tb_FollowerDate").val(now);

    }
